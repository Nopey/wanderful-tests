mapsize 100 ;
start

    create Bob 32 32 north ;

    % examples, assuming Bob has been created
    int abc ;
    abc := row ( Bob ) ;
    abc := 27 ;
    print abc ;
    name whoever ;
    whoever := Bob ;
    str text ;
    text := 'hello!' ;
    str moretext ;
    moretext := text ;

    facing mydir ;
    mydir := south ;
    mydir := face ( Bob ) ;

    % Woops!
    turn Bob abc ;

finish
